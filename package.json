{
	"name": "wallet",
	"version": "1.40.3",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"ionic:build": "npm run build",
		"ionic:serve": "npm run dev -- --host",
		"ios": "ionic cap run ios -l --external",
		"android": "ionic cap run android -l --external",
		"apk": "ionic capacitor copy android && cd android && ./gradlew assembleDebug && ./gradlew installDebug && cd ..",
		"web": "ionic serve",
		"icons": "npx capacitor-assets generate --ios --android",
		"postinstall": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@capacitor/assets": "^3.0.4",
		"@faker-js/faker": "^8.4.1",
		"@inlang/paraglide-js": "1.3.2",
		"@lottiefiles/svelte-lottie-player": "^0.3.1",
		"@playwright/test": "^1.42.1",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^11.1.0",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.2.6",
		"@semantic-release/npm": "^11.0.3",
		"@semantic-release/release-notes-generator": "^12.1.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "8.56.5",
		"@types/lodash": "^4.17.0",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"@vite-pwa/sveltekit": "^0.4.0",
		"autoprefixer": "^10.4.18",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"semantic-release": "^23.0.2",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.7",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"vite": "^5.1.7",
		"vitest": "^1.3.1"
	},
	"type": "module",
	"dependencies": {
		"@aparajita/capacitor-biometric-auth": "^7.1.1",
		"@capacitor-mlkit/barcode-scanning": "^5.4.0",
		"@capacitor/android": "^5.7.2",
		"@capacitor/app": "^5.0.7",
		"@capacitor/camera": "^5.0.9",
		"@capacitor/cli": "^5.7.2",
