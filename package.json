		"@inlang/paraglide-js-adapter-sveltekit": "^0.5.10",
		"@ionic/core": "^7.8.0",
		"@slangroom/core": "1.30",
		"@slangroom/helpers": "^1.30",
		"@slangroom/http": "1.30",
