    "@ionic/core": "^7.8.6",
    "@slangroom/core": "~1.30.0",
    "@slangroom/helpers": "^1.32.1",
    "@slangroom/http": "~1.30.0",
    "@slangroom/pocketbase": "~1.31.0",
    "@slangroom/zencode": "^1.32.1",
    "ajv": "^8.16.0",
    "ajv-formats": "^2.1.1",
    "capacitor-native-settings": "^6.0.0",
    "dayjs": "^1.11.11",
    "did-resolver": "^4.1.0",
    "ionic-svelte": "^0.5.83",
    "ionicons": "^7.4.0",
    "lodash": "^4.17.21",
    "sveltekit-superforms": "1.13.4",
